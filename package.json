{
  "name": "Spell Check Whole Page",
  "description": "Checks spelling your entire page including symbols and overrides",
  "version": "1.3.2",
  "engines": {
    "sketch": ">=49.0"
  },
  "repository": "github.com/Tallwave/sketch-spellcheck-all-layers",
  "skpm": {
    "name": "Spell Check Whole Page",
    "manifest": "src/manifest.json",
    "main": "SpellCheckWholePage.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  }
}
